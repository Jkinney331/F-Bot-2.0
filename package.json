{
  "name": "f-bot-2.0",
  "version": "2.0.0",
  "description": "Enhanced Fascia AI Chatbot with HIPAA compliance and multi-LLM orchestration",
  "main": "src/index.js",
  "type": "module",
  "keywords": [
    "ai",
    "chatbot",
    "fascia",
    "medical",
    "hipaa",
    "flowise",
    "healthcare",
    "ultrasound",
    "diagnosis"
  ],
  "author": "F-Bot Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jkinney331/F-Bot-2.0.git"
  },
  "bugs": {
    "url": "https://github.com/Jkinney331/F-Bot-2.0/issues"
  },
  "homepage": "https://github.com/Jkinney331/F-Bot-2.0#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run build:flowise && npm run build:config",
    "build:flowise": "cp -r flowise-config/* node_modules/flowise/packages/components/nodes/",
    "build:config": "node scripts/build-config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "migrate": "npm run migrate:up",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "seed": "node scripts/seed.js",
    "docker:build": "docker build -t fbot:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy": "npm run build && docker-compose -f deployment/enhanced-deployment.yml up -d",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:licenses": "license-checker --summary",
    "docs:generate": "jsdoc src/ -d docs/api/",
    "health:check": "node scripts/health-check.js",
    "install:flowise": "./setup-scripts/install-flowise.sh",
    "setup": "npm run install:flowise && npm run migrate && npm run seed",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install",
    "start:full": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "scrape:test": "node backend/scripts/test-scraping.js",
    "scrape:medical": "node backend/scripts/scrape-medical-data.js",
    "init-sources": "node backend/scripts/initializeMedicalSources.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "redis": "^4.6.11",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "joi": "^17.11.0",
    "uuid": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "node-cache": "^5.1.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "prom-client": "^15.0.0",
    "openai": "^4.20.1",
    "langfuse": "^3.0.0",
    "flowise-sdk": "^1.0.0",
    "concurrently": "^8.2.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.2",
    "bull": "^4.12.0",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.1.0",
    "jsdoc": "^4.0.2",
    "license-checker": "^25.0.1",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0"
  },
  "peerDependencies": {
    "flowise": "^1.4.0"
  },
  "optionalDependencies": {
    "tensorflow": "^4.13.0",
    "sharp": "^0.32.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/tests/**",
      "!src/scripts/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
      "node": true,
      "es2022": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Jkinney331"
  }
} 